{
  "threshold": 5,
  "reporters": ["console", "json"],
  "output": ".jscpd-report",
  "ignore": [
    "**/node_modules/**",
    "**/lib/**",
    "**/test/**",
    "**/masonry.pkgd.min.js",
    "**/iframe-resizer.child.js",
    "**/reviews-embed.js",
    "**/reviews/**",
    "**/package-lock.json"
  ],
  "format": ["javascript"],
  "absolute": false,
  "gitignore": true,
  "minLines": 1,
  "minTokens": 40
}
